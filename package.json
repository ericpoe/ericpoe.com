{
  "name": "ericpoe.com",
  "description": "Code and content for ericpoe.com",
  "author": "Eric Poe <eric.poe@gmail.com>",
  "license": "MIT",
  "scripts": {
    "css": "node_modules/.bin/tailwind build ./src/components/tailwind.css -c ./tailwind.config.js -o ./src/components/index.css",
    "build": "yarn css && purgecss -c ./purgecss.config.js -o ./src/components && gatsby build",
    "develop": "yarn css && gatsby develop",
    "develop:debug": "node --inspect=127.0.0.1:9232 node_modules/.bin/gatsby develop",
    "format": "prettier --write '**/*.{js,json,jsx}'",
    "lint": "eslint '**/*.{js,jsx}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "gatsby": "^4.3.0",
    "gatsby-plugin-google-analytics": "^4.3.0",
    "gatsby-plugin-manifest": "^4.3.0",
    "gatsby-plugin-offline": "^5.3.0",
    "gatsby-plugin-react-helmet": "^5.3.0",
    "gatsby-plugin-sharp": "^4.3.0",
    "gatsby-plugin-sitemap": "^5.3.0",
    "gatsby-remark-images": "^6.3.0",
    "gatsby-remark-prismjs": "^6.3.0",
    "gatsby-source-filesystem": "^4.3.0",
    "gatsby-transformer-remark": "^5.3.0",
    "prism-themes": "^1.6.0",
    "prismjs": "^1.23.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-gatsby-standard": "^2.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.2.1",
    "purgecss": "^2.1.2",
    "tailwindcss": "^1.1"
  }
}
